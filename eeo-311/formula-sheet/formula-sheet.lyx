\begin_layout Standard

\size larger

\begin_inset Formula $\int kf(u) du = k \int f(u) du + C$
\end_inset

, 
\begin_inset Formula $\int [f(u) \pm g(u)]du = \int f(u) du \pm \int g(u) du$
\end_inset

, 
\begin_inset Formula $\int du = u +C$
\end_inset

, 
\begin_inset Formula $\int u^n du = \frac{u^{n+1}}{n+1} +C, n \neq -1$
\end_inset

, 
\begin_inset Formula $\int \frac{du}{u} = \ln |u| +C$
\end_inset

, 
\begin_inset Formula $\int \frac{u}{du} = \frac{u^2}{2} +C$
\end_inset

, 
\begin_inset Formula $\int e^u du = e^u +C$
\end_inset

,
\begin_inset Formula $\int e^{4u} = \frac{e^{4u}}{4} +C$
\end_inset

, 
\begin_inset Formula $\int a^u du = \left( \frac{1}{\ln a} \right) a^u +C$
\end_inset

, 
